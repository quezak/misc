{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "typescript.preferences.quoteStyle": "single",
  "editor.fontFamily": "'Monoid'",
  "editor.fontSize": 11,
  "vim.useSystemClipboard": true,
  "editor.tokenColorCustomizations": {
    "comments": "#5b7baa",
    "strings": "#c6635d",
    "types": "#ffffff",
    "functions": "#ffc66d",
    "keywords": "#70b230",
    "numbers": "#bb632e",
    "variables": "#a9b7c6",
    "textMateRules": [
      {
        "scope": "support.type.property-name",
        "settings": { "foreground": "#ffc66d"}
      },
      {
        "scope": "meta.type.annotation",
        "settings": { "foreground": "#cc7832" }
      },
      {
        "scope": "storage.modifier",
        "settings": { "foreground": "#70b230" }
      },
      {
        "scope": "comment.block.documentation",
        "settings": { "foreground": "#5f64aa" }
      },
      {
        "scope": "variable.parameter",
        "settings": { "foreground": "#34bb90" }
      },
      {
        "scope": "variable.language.this, variable.language.super",
        "settings": { "foreground": "#70b230", "fontStyle": "bold" }
      },
      {
        "scope": "constant.language.boolean",
        "settings": { "foreground": "#bb632e" }
      },
      {
        "scope": "constant.language.null, constant.language.undefined",
        "settings": { "foreground": "#bb2c17" }
      },
      {
        "scope": "meta.object-literal.key",
        "settings": { "foreground": "#42cec7"}
      },
    ]
  },
  "workbench.colorCustomizations": {
    "editor.foreground": "#a9b7c6",
    "editor.lineHighlightBackground": "#2b2b2b",
    "editorGutter.background": "#2b2b2b",
    "editorLineNumber.foreground": "#ad8d4b",
    "editorLineNumber.activeForeground": "#e6b663",
  },
  "files.autoSave": "off",
  "window.zoomLevel": 0,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-r>": false,
    "<C-z>": false,
    "<C-y>": false,
    "<C-c>": false,
    "<C-x>": false,
    "<C-v>": false,
    "<C-b>": false,
    "<C-o>": false,
    "<C-k>": false,
  },
  "solidity.linter": "solium",
  "solidity.soliumRules": {
      "quotes": ["error", "double"],
      "indentation": ["error", 4]
  },
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.blame.format": "${author|10?} ${agoOrDate|14-}",
  "gitlens.blame.dateFormat": "YYYY-MM-DD",
  "editor.minimap.maxColumn": 100,
  "editor.minimap.showSlider": "always",
  "todo-tree.defaultHighlight": {
    "background": "#ffc66d",
    "foreground": "black",
    "type": "tag"
  },
  "todo-tree.tags": ["TODO", "FIXME", "XXX", "NOTE", "TBD"],
  "todo-tree.customHighlight": {
    "TODO": { "icon": "check", "iconColour": "#ffc66d", "background": "#ffc66d" },
    "TBD": { "icon": "check", "iconColour": "#ffc66d", "background": "#ffc66d" },
    "FIXME": { "icon": "beaker", "iconColour": "#bb632e", "background": "#bb632e" },
    "XXX": { "icon": "alert", "iconColour": "#bb2c17", "background": "#bb2c17" },
    "NOTE": { "icon": "note", "iconColour": "#70b230", "background": "#70b230" },
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "tslint.configFile": "tslint.json",
  "editor.fontLigatures": true,
  "search.showLineNumbers": true,
  "workbench.editor.highlightModifiedTabs": true,
  "tslint.ignoreDefinitionFiles": false,
  "importSorter.importStringConfiguration.numberOfEmptyLinesAfterAllImports": 2,
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.sortConfiguration.customOrderingRules.defaultOrderLevel": 90,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 100,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimit",
  "importSorter.generalConfiguration.exclude": [
    "types/.*\\.d\\.ts",
  ],
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      // These are the 'side-effect-only' imports without `from`
      "type": "importMember",
      "regex": "^$",
      "orderLevel": 10,
      "disableSort": true
    },
    {
      // Non-namespace package imports
      "regex": "^[^.@]",
      "orderLevel": 20
    },
    {
      // Aliased local imports
      "regex": "^@(auth|dto|entities|utils|generated|app|contracts|common|backend-common)/",
      "orderLevel": 30
    },
    {
      // Namespaced package imports
      "regex": "^[@]",
      "orderLevel": 25
    },
    {
      // Local imports
      "regex": "^[.]",
      "orderLevel": 40
    },
  ],
  "python.linting.flake8Enabled": true,
  "python.jediEnabled": false,
  "files.exclude": {
    "**/node_modules": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "importSorter.generalConfiguration.sortOnBeforeSave": true
}
