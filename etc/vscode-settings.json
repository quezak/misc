{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "typescript.preferences.quoteStyle": "single",
  "editor.fontFamily": "Monoid",
  "editor.fontSize": 12,
  "vim.useSystemClipboard": true,
  "vim.textwidth": 100,
  "editor.tokenColorCustomizations": {
    "comments": "#5b7baa",
    "strings": "#c6635d",
    "types": "#ffffff",
    "functions": "#ffc66d",
    "keywords": "#70b230",
    "numbers": "#bb632e",
    "variables": "#a9b7c6",
    "textMateRules": [
      {
        "scope": "support.type.property-name",
        "settings": { "foreground": "#ffc66d" }
      },
      {
        "scope": "meta.type.annotation",
        "settings": { "foreground": "#cc7832" }
      },
      {
        "scope": "storage.modifier",
        "settings": { "foreground": "#70b230" }
      },
      {
        "scope": "comment.block.documentation",
        "settings": { "foreground": "#5f64aa" }
      },
      {
        "scope": "variable.parameter",
        "settings": { "foreground": "#34bb90" }
      },
      {
        "scope": "variable.language.this, variable.language.super",
        "settings": { "foreground": "#70b230", "fontStyle": "bold" }
      },
      {
        "scope": "constant.language.boolean",
        "settings": { "foreground": "#bb632e" }
      },
      {
        "scope": "constant.language.null, constant.language.undefined",
        "settings": { "foreground": "#bb2c17" }
      },
      {
        "scope": "meta.object-literal.key",
        "settings": { "foreground": "#42cec7" }
      },
      {
        "scope": "variable.other.normal.shell",
        "settings": { "foreground": "#9077b4" }
      },
      {
        "scope": "variable.other.special.shell",
        "settings": { "foreground": "#9077b4" }
      }
    ],
  },
  "workbench.colorCustomizations": {
    "editor.foreground": "#a9b7c6",
    "editor.lineHighlightBackground": "#2b2b2b",
    "editorGutter.background": "#2b2b2b",
    "editorLineNumber.foreground": "#ad8d4b",
    "editorLineNumber.activeForeground": "#e6b663",
    "editorBracketHighlight.foreground1": "#f4a460",
    "editorBracketHighlight.foreground2": "#66ddaa",
    "editorBracketHighlight.foreground3": "#DDA0DD",
    "editorIndentGuide.activeBackground": "#85ba5d",
    "editorBracketMatch.border": "#53664e",
    "editorBracketMatch.background": "#53664e",
  },
  "files.autoSave": "off",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-f>": false,
    "<C-h>": false,
    "<C-k>": false,
    "<C-o>": false,
    "<C-p>": false,
    "<C-r>": false,
    "<C-v>": false,
    "<C-w>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-z>": false,
    "<C-space>": false,
  },
  "solidity.linter": "solhint",
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.blame.format": "${author|10?} ${agoOrDate|14-}",
  "gitlens.blame.dateFormat": "YYYY-MM-DD",
  "editor.minimap.maxColumn": 100,
  "editor.minimap.showSlider": "always",
  "todo-tree.highlights.defaultHighlight": {
    "background": "#ffc66d",
    "foreground": "black",
    "type": "tag"
  },
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "XXX",
    "NOTE",
    "TBD",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "iconColour": "#ffc66d",
      "background": "#ffc66d"
    },
    "TBD": {
      "icon": "check",
      "iconColour": "#ffc66d",
      "background": "#ffc66d"
    },
    "FIXME": {
      "icon": "beaker",
      "iconColour": "#bb632e",
      "background": "#bb632e"
    },
    "XXX": {
      "icon": "alert",
      "iconColour": "#bb2c17",
      "background": "#bb2c17"
    },
    "NOTE": { "icon": "note", "iconColour": "#70b230", "background": "#70b230" }
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "editor.fontLigatures": true,
  "search.showLineNumbers": true,
  "workbench.editor.highlightModifiedTabs": true,
  "python.linting.flake8Enabled": true,
  "python.jediEnabled": false,
  "files.exclude": {
    "**/node_modules": true
  },
  "editor.minimap.enabled": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.rulers": [100],
  "typescript.tsserver.log": "off",
  "editor.find.autoFindInSelection": "multiline",
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "workbench.list.horizontalScrolling": true,
  "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
  "bracket-pair-colorizer-2.colorMode": "Consecutive",
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.colors": ["SandyBrown", "MediumAquaMarine", "Plum"],
  "todo-tree.tree.showScanModeButton": false,
  "python.insidersChannel": "off",
  "customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.font.regular": "DejaVu Sans Condensed",
  "python.languageServer": "Default",
  "gitlens.currentLine.enabled": false,
  "gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true,
    "suppressIntegrationRequestTimedOutWarning": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "eslint.options": {
    "overrideConfig": {
      "plugins": ["only-warn"]
    }
  },
  "openapi.securityAuditToken": "CiBb/4Ce9O95t06m9uO4smaLgGQRScftX/ddcllG0njZkBIYXlfd1ZvtYDP5OHdCN+ccpU7W+c7xutwiGnPlBZvirHB+huUigKfgnnPoGrHn354wbB5FwCWnTVm2UF54FfPXy8L4LvA3zpogQ3m1e8R4ODgnbVxZ4MpRrHrnbRS/TiZ6HUZrIChzvPMNo5sDWUbcLByxwnF/WL6uTrgYIlFKu9aJT34IHnVtonozr9BF",
  "editor.bracketPairColorization.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": false,
  "redhat.telemetry.enabled": false,
  "editor.inlayHints.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.files.associations": {
    "*.type-spec.ts": "Test-ts",
    "*.e2e-spec.ts": "Test-ts",
  },
  "jest.shell": "/bin/bash",
  "jest.jestCommandLine": "source ~/.nvm/nvm.sh && yarn env-cmd -f ../../.env run test",
  "jest.autoRun": "off",
  "git.autofetch": true,
  "editor.stickyScroll.enabled": true,
  "githubPullRequests.pullBranch": "never",
  "material-icon-theme.activeIconPack": "nest",
  "eslint.run": "onSave",
  "stylelint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "terminal.external.linuxExec": "mate-terminal",
  "terminal.integrated.fontSize": 12,
  "vim.statusBarColorControl": false,
  "vim.foldfix": false,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "diffEditor.renderSideBySide": true,
  "files.simpleDialog.enable": true,
  "diffEditor.ignoreTrimWhitespace": false
}
